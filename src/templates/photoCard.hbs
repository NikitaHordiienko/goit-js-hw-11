{{#each this}}
   <div class="photo-card">
        <a href="{{this.largeImageURL}}">
            <img src="{{this.webformatURL}}" alt="{{this.tags}}" loading="lazy" class="photo"/>
        </a>
        <div class="info">
            <p class="info-item">
             <b>Likes</b><span class="info-item__span">{{this.likes}}</span>
            </p>
            <p class="info-item">
             <b>Views</b><span class="info-item__span">{{this.views}}</span>
            </p>
            <p class="info-item">
             <b>Comments</b><span class="info-item__span">{{this.comments}}</span>
            </p>
            <p class="info-item">
             <b>Downloads</b><span class="info-item__span">{{this.downloads}}</span>
            </p>
        </div>
    </div>
{{/each}}